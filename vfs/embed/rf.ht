<html><head>
<title>Ethersex - RFM12 Control</title>
<script src="scr.js" type="text/javascript"></script>
<script type="text/javascript">
function ecmd_set_rf1(type, code, delay, repeat) {
   var url = '/ecmd?rfm12+' + type + '+' + code + '+' + delay + '+' + repeat;
   ArrAjax.aufruf(url);
}
function ecmd_set_rf2(type, code, cmd, delay, repeat) {
   var url = '/ecmd?rfm12+' + type + '+' + code + '+' + cmd + '+' + delay + '+' + repeat;
   ArrAjax.aufruf(url);
}
</script>
</head>
<body>
<h1>RFM12 Powerswitch</h1>
<table> 
<tr>
 <td>Switch</td>
 <td>Command</td>
</tr>
<tr><th colspan="2">2272</th></tr>
<tr><td>A</td><td><a href="javascript:ecmd_set_rf1(2272,'0,5,81',76,10)">ON</a> <a href="javascript:ecmd_set_rf1(2272,'0,5,84',76,10)">OFF</a></td></tr>
<tr><td>B</td><td><a href="javascript:ecmd_set_rf1(2272,'0,17,81',76,10)">ON</a> <a href="javascript:ecmd_set_rf1(2272,'0,17,84',76,10)">OFF</a></td></tr>
<tr><td>C</td><td><a href="javascript:ecmd_set_rf1(2272,'0,20,81',76,10)">ON</a> <a href="javascript:ecmd_set_rf1(2272,'0,20,84',76,10)">OFF</a></td></tr>

<tr><th colspan="2">tevion</th></tr>
</table>

<tr><td>Dose 1</td><td>
