CONTROL_START
  
  PIN_INPUT(TASTER)
  PIN_PULLUP(TASTER)
  PIN_OUTPUT(LED)

  ACTION(new)
      TIMER_START(new)
      PIN_SET(LED);
      TIMER_WAIT(new, 20)
      PIN_CLEAR(LED);
  ACTION_END(new)

  ON PIN_FALLING(TASTER) DO ACTION_RESTART(new) END
  
  ON PIN_FALLING(TASTER) DO(new2)
      TIMER_START(new2)
      PIN_SET(LED);
      TIMER_WAIT(new2, 20)
      PIN_CLEAR(LED);
  END (new2)

CONTROL_END

